SQL CODE:
CREATE TABLE ROOM(
    BEDOPT VARCHAR(20),
    KITCHOPT VARCHAR(20),
    COFFEEOPT VARCHAR(20),
    ACCOPT VARCHAR(20),
    ROOMNUM int,
    ROOMPRICE int
);
CREATE TABLE BOOKEDROOM(
    BOOKINGID int,
    BOOKEDGUEST VARCHAR(10),
    BOOKEDROOM int,
    CHECKINYEAR int,
    CHECKINDAY int,
    CHECKOUTDAY int
);
CREATE TABLE BOOKING(
    BOOKINGID int,
    BOOKEDGUEST VARCHAR(10)
);
CREATE TABLE GUEST(
    GUESTUSERNAME VARCHAR(15),
    GUESTPASS VARCHAR(15),
    GUESTNAME VARCHAR(15)
);
CREATE TABLE EMPLOYEE(
    EMPUSERNAME VARCHAR(15),
    EMPPASS VARCHAR(15),
    EMPNAME VARCHAR(15)
);
